#include <stdio.h>
#include <stdlib.h>

float nota;

int main(int argc, char *argv[]) {
	
printf("Informe a nota do aluno: ");
    scanf("%f", &nota);

    if (nota >= 9 && nota <= 10) {
        printf("A");
    } else if (nota >= 7 && nota < 9) {
        printf("B");
    } else if (nota >= 5 && nota < 7) {
        printf("C");
    } else if (nota >= 4 && nota < 5) {
        printf("D");
    } else if (nota >= 0 && nota < 4) {
        printf("F");
    } else {
        printf("Nota invalida");
    }
	
	return 0;
}
